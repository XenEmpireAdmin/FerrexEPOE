<?xml version="1.0" encoding="UTF-8"?>

<ThingDefs>

   <ThingDef ParentName="BodyPartNaturalBase">
      <defName>FrxTail</defName>
      <label>Ferrex tail</label>
      <description>A Ferrex tail.</description>
      <graphicData>
         <texPath>Things/Item/BodyPart/Organ</texPath>
         <graphicClass>Graphic_Single</graphicClass>
      </graphicData>
      <statBases>
         <MarketValue>200</MarketValue>
      </statBases>
   </ThingDef>
   

  <ThingDef ParentName="BodyPartArtificialBase">
    <defName>FrxClothTail</defName>
    <label>cloth Ferrex tail</label>
    <description>Simple prosthesis made to replace a missing tail. It sucks! It doesn't feel right!</description>
    <graphicData>
      <texPath>Things/Item/BodyPart/ArtificialOrgan</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>400</MarketValue>
      <Mass>.3</Mass>
    </statBases>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
  </ThingDef>

  <ThingDef ParentName="BodyPartArtificialBase">
    <defName>FrxBionicTail</defName>
    <label>Ferrex bionic tail</label>
    <description>A masterpiece of bionics! Equipped with special bio-feedback sensors and micro-gyroscopes, and many other hyphenated technical components!</description>
    <graphicData>
      <texPath>Things/Item/BodyPart/ArtificialOrgan</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>1500</MarketValue>
      <Mass>1</Mass>
    </statBases>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
  </ThingDef>

